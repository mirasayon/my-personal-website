{
    "compilerOptions": {
        "tsBuildInfoFile": "./logs/build.tsbuildinfo",
        "lib": ["dom", "dom.iterable", "esnext"],
        "allowJs": true,
        "skipLibCheck": true,
        "strict": true,
        "noEmit": true,
        "esModuleInterop": true,
        "module": "esnext",
        "moduleResolution": "bundler",
        "resolveJsonModule": true,
        "isolatedModules": true,
        "jsx": "preserve",
        "incremental": true,
        "plugins": [
            {
                "name": "next"
            }
        ],
        "paths": {
            "#/*": ["./src/*"],
            "%/*": ["./src/components/*"],
            "#configs/*": ["./src/configs/*"],
            "#consts/*": ["./src/constants/*"],
            "#css/*": ["./src/styles/*"]
        },
        "target": "ESNext"
    },
    "include": ["next-env.d.ts", "./src/**/*", ".next/types/**/*.ts"],
    "exclude": ["node_modules"]
}
